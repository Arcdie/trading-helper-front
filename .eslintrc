{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  "rules": {
    "no-useless-return": 0,
    "no-param-reassign": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "no-else-return": 0,
    "no-restricted-syntax": 0,
    "arrow-body-style": 0,
    "no-underscore-dangle": 0,
    "global-require": 0,
    "import/no-dynamic-require": 0,
    "func-names": 0,
    "one-var": 0,
    "no-use-before-define": 0,
    "no-restricted-globals": 0,
    "one-var-declaration-per-line": 0,
    "no-multi-str": 0,
    "prefer-destructuring": 0,
    "guard-for-in": 0,
    "linebreak-style": 0,
    "import/no-unresolved": 0,
    "no-plusplus": 0,
    "no-multi-assign": 0,
    "no-bitwise": 0,
    "no-continue": 0,
    "arrow-parens": 0,
    "function-paren-newline": 0,
    "no-unused-expressions": 0,
    "no-loop-func": 0,
    "no-await-in-loop": 0,
    "no-unreachable-loop": 0,
    "no-confusing-arrow": 0,
    "max-len": 0,
    "no-lonely-if": 0,
    "max-classes-per-file": 0,
    "class-methods-use-this": 0
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": 'module',
  },
}
