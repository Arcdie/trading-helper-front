extends ../app

block variables
	-
		const pageTitle = 'Статистика для плотностей';

block content
	.page.volume-statistics
		.charts-container

		.instruments-container
			.search
				input(type="text" placeholder="Поиск")
		
			.instruments-list
				.headlines
					span.instrument-name Инструмент
					span.amount-volume К-во

				.list

block scripts
	script(src='/libs/moment.min.js')
	script(src='/libs/trading-view-charts.min.js')

	script(src='/javascripts/websocket.js')
	script(src='/javascripts/chart-functions/chart-candles.js')
	script(src='/javascripts/chart-functions/chart-volume.js')

	script(src='/javascripts/web/volume-statistics.js')
