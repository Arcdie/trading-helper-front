.row
  .trades
    .trading-statistics
      table
        tr
          th Профит
          th Сделки
          th Комиссии
          th Мин. профит
          th Макс. профит
      
        tr
          td.profit
            span 0
            | $

          td.number-trades
            span.win 0
            | /
            span.lose 0
            | (
            span.relation 0
            | )
        
          td.sum-commissions
            span 0
            | $

          td.min-profit
            span 0
            | $
            
          td.max-profit
            span 0
            | $
        

    .trading-list
      table
        tr
          th #
          th К-во трейдов
          th Профит
          th %
          th Тип
          th Статус
          th Комиссия
          th Цена покупки
          th Цена продажи
          th Дата входа
          th Дата выхода        
